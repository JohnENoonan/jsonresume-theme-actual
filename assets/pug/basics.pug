div(class='basics')
  div(class='table')
    div(class='main')
      span(class='name')=resume.basics.name
      span(class='label')=resume.basics.label
    div(class='side')
      ul(class='contact')
        li(class='article')
            !=helper.beautifyLink(resume.basics.url)
        li=resume.basics.email
        li=helper.calcLocation(resume.basics.location)
  if resume.basics.summary
    div(class='summary markdown')!=helper.mdToHtml(resume.basics.summary)
